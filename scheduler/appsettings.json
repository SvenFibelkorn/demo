{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "OpenTelemetry": {
    "Endpoint": "http://localhost:4317"
  },
  "TickerQ": {
    "InstanceName": "tickerq:scheduler:"
  },
  "Scheduler": {
    "CronExpression": "0 0 * * * *",
    "EmbeddingCronExpression": "0 * * * * *",
    "EmbeddingBatchSize": 200,
    "RunOnceOnStartup": true,
    "FeedListFiles": [
      "../corpus/economist.txt",
      "../corpus/theverge.txt",
      "../corpus/zeit.txt"
    ]
  },
  "Embeddings": {
    "Provider": "local",
    "VectorSize": 768,
    "Local": {
      "ModelPath": "../models/bge-base-en-v1.5",
      "OnnxModelPath": "onnx/model.onnx",
      "TokenizerPath": "vocab.txt",
      "Lowercase": true,
      "MaxLength": 512
    },
    "Ollama": {
      "Endpoint": "http://localhost:11434",
      "Model": "bge-base-en-v1.5",
      "TimeoutSeconds": 60
    }
  }
}
